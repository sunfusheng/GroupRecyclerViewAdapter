<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.sunfusheng.adapter.sample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".InsertRemoveUpdateActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".ExpandCollapseActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".StickyActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".WeChatMeActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".WeChatNewMessageActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".BottomSheetDialogActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>